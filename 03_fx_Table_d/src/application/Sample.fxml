<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="application.SampleController">
	<center>
		<TableView fx:id="tableView" prefHeight="569.0" prefWidth="728.0"
			BorderPane.alignment="CENTER">
			<columns>
				<TableColumn prefWidth="139.0" text="Vorname">
					<cellValueFactory>
						<PropertyValueFactory property="vorname" />

					</cellValueFactory>
				</TableColumn>
				<TableColumn prefWidth="147.0" text="Nachname">
					<cellValueFactory>
						<PropertyValueFactory property="nachname" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn prefWidth="147.0" text="Alter">
					<cellValueFactory>
						<PropertyValueFactory property="alter" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
	</center>
</BorderPane>
